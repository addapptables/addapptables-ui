{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "ordered-imports": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-multiline-js": false
  }
}